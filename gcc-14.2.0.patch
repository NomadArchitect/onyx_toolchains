Index: gcc-14.2.0/gcc/config/onyx.h
===================================================================
--- /dev/null
+++ gcc-14.2.0/gcc/config/onyx.h
@@ -0,0 +1,109 @@
+#undef TARGET_OS_CPP_BUILTINS
+#define TARGET_OS_CPP_BUILTINS()                                               \
+  do {                                                                         \
+    builtin_define_std("onyx");                                                \
+    builtin_define_std("unix");                                                \
+    builtin_assert("system=onyx");                                             \
+    builtin_assert("system=unix");                                             \
+    builtin_assert("system=posix");                                            \
+  } while (0);
+
+#define ONYX_EXTRA_STARTFILE_SPEC ""
+#define ONYX_EXTRA_ENDFILE_SPEC ""
+
+#undef STARTFILE_SPEC
+/* Onyx uses crt1.o, crti.o and crtbegin.o as start files. If we're a shared
+ * library or pie, use crtbeginS.o and Scrt1.o */
+//#define STARTFILE_SPEC "%{!shared: %{" PIE_SPEC ": Scrt1.o%s;: crt1.o%s}}
+// crti.o%s %{shared|" PIE_SPEC ": crtbeginS.o%s;: crtbegin.o%s}"
+#define STARTFILE_SPEC                                                         \
+  "%{!shared:%{" PIE_SPEC ":S}crt1.o%s} crti.o%s %{shared|" PIE_SPEC           \
+  ":crtbeginS.o%s;:crtbegin.o%s}" ONYX_EXTRA_STARTFILE_SPEC
+
+#undef ENDFILE_SPEC
+/* Onyx uses crtend.o and crtn.o as end files. If we're a shared library or pie,
+ * use crtendS.o */
+#define ENDFILE_SPEC                                                           \
+  "%{shared|" PIE_SPEC                                                         \
+  ": crtendS.o%s;: crtend.o%s} crtn.o%s" ONYX_EXTRA_ENDFILE_SPEC
+
+// re-define in architecture-specific target files
+#define LD_ONYX_PATH "/lib/ld-onyx.so"
+/* Onyx uses /lib/ld-onyx.so as a dynamic linker; also, build shared objects by
+ * default */
+#define ONYX_TARGET_LINK_SPEC ""
+#undef LINK_SPEC
+#define LINK_SPEC                                                              \
+  ONYX_TARGET_LINK_SPEC                                                        \
+  "%{shared: -shared} %{static: -static}"                                      \
+  "%{!shared: %{!static: %{rdynamic: -export-dynamic} %{!dynamic-linker: "     \
+  "-dynamic-linker " LD_ONYX_PATH "}}}"                                        \
+  " -z combreloc"                                                              \
+  " -z relro"                                                                  \
+  " -z now"                                                                    \
+  " -z text"                                                                   \
+  " -z noexecstack"                                                            \
+  "%{!hash-style: --hash-style=gnu}"                                           \
+  "%{!no-build-id: --build-id}"                                                \
+  "%{!no-eh-frame-hdr: --eh-frame-hdr}"
+
+/* Link with libpthread if -pthread and link with libc by default */
+#undef LIB_SPEC
+#define LIB_SPEC "%{pthread: -lpthread} -lc"
+
+#if defined(HAVE_LD_EH_FRAME_HDR)
+#define LINK_EH_SPEC "%{!static|static-pie: --eh-frame-hdr} "
+#endif
+
+#define ONYX_TARGET_LINK_GCC_C_SEQUENCE_SPEC                                   \
+  "%{static|static-pie:--start-group} %G %{!nolibc:%L} \
+   %{static|static-pie:--end-group}%{!static:%{!static-pie:%G}}"
+
+#undef LINK_GCC_C_SEQUENCE_SPEC
+#define LINK_GCC_C_SEQUENCE_SPEC ONYX_TARGET_LINK_GCC_C_SEQUENCE_SPEC
+
+/* Link -lasan early on the command line.  For -static-libasan, don't link
+   it for -shared link, the executable should be compiled with -static-libasan
+   in that case, and for executable link with --{,no-}whole-archive around
+   it to force everything into the executable.  And similarly for -ltsan,
+   -lhwasan, and -llsan.  */
+#if defined(HAVE_LD_STATIC_DYNAMIC)
+#undef LIBASAN_EARLY_SPEC
+#define LIBASAN_EARLY_SPEC                                                     \
+  "%{!shared:libasan_preinit%O%s} "                                            \
+  "%{static-libasan:%{!shared:" LD_STATIC_OPTION                               \
+  " --whole-archive -lasan --no-whole-archive " LD_DYNAMIC_OPTION              \
+  "}}%{!static-libasan:-lasan}"
+#undef LIBHWASAN_EARLY_SPEC
+#define LIBHWASAN_EARLY_SPEC                                                   \
+  "%{static-libhwasan:%{!shared:" LD_STATIC_OPTION                             \
+  " --whole-archive -lhwasan --no-whole-archive " LD_DYNAMIC_OPTION            \
+  "}}%{!static-libhwasan:-lhwasan}"
+#undef LIBTSAN_EARLY_SPEC
+#define LIBTSAN_EARLY_SPEC                                                     \
+  "%{!shared:libtsan_preinit%O%s} "                                            \
+  "%{static-libtsan:%{!shared:" LD_STATIC_OPTION                               \
+  " --whole-archive -ltsan --no-whole-archive " LD_DYNAMIC_OPTION              \
+  "}}%{!static-libtsan:-ltsan}"
+#undef LIBLSAN_EARLY_SPEC
+#define LIBLSAN_EARLY_SPEC                                                     \
+  "%{!shared:liblsan_preinit%O%s} "                                            \
+  "%{static-liblsan:%{!shared:" LD_STATIC_OPTION                               \
+  " --whole-archive -llsan --no-whole-archive " LD_DYNAMIC_OPTION              \
+  "}}%{!static-liblsan:-llsan}"
+#endif
+
+/* Use --as-needed -lgcc_s for eh support.  */
+#ifdef HAVE_LD_AS_NEEDED
+#define USE_LD_AS_NEEDED 1
+#endif
+
+#undef OBJECT_FORMAT_ELF
+#define OBJECT_FORMAT_ELF
+
+#undef CPLUSPLUS_CPP_SPEC
+#define CPLUSPLUS_CPP_SPEC "-D_GNU_SOURCE %(cpp)"
+
+#ifndef TARGET_ASM_FILE_END
+#define TARGET_ASM_FILE_END file_end_indicate_exec_stack
+#endif
