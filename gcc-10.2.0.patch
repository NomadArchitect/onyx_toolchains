diff -Naur gcc-10.2.0/gcc/config/i386/onyx64.h gcc-10.2.0-patched/gcc/config/i386/onyx64.h
--- gcc-10.2.0/gcc/config/i386/onyx64.h	1970-01-01 01:00:00.000000000 +0100
+++ gcc-10.2.0-patched/gcc/config/i386/onyx64.h	2021-05-18 00:31:54.650679616 +0100
@@ -0,0 +1,21 @@
+/* Taken verbatim from gnu-user64.h
+ * I didn't simplify the expressions below because we might want x32 or 32-bit compatibility
+ * support some day.
+ */
+
+#undef TARGET_64BIT
+#undef TARGET_X32
+#undef TARGET_THREAD_SSP_OFFSET
+#undef TARGET_THREAD_SPLIT_STACK_OFFSET
+
+#define TARGET_64BIT 1
+#define TARGET_X32 0
+
+#define TARGET_THREAD_SSP_OFFSET \
+  (TARGET_64BIT ? (TARGET_X32 ? 0x18 : 0x28) : 0x14)
+
+/* i386 glibc/musl provides __private_ss in %gs:0x30.
+   x32 glibc/musl provides it in %fs:0x40.
+   x86_64 glibc/musl provides it in %fs:0x70.  */
+#define TARGET_THREAD_SPLIT_STACK_OFFSET \
+  (TARGET_64BIT ? (TARGET_X32 ? 0x40 : 0x70) : 0x30)
